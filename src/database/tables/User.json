{
  "field": {
    "id": {
      "type": "INT",
      "constraint": ["UNSIGNED", "AUTO_INCREMENT"]
    },
    "email": {
      "type": "VARCHAR(320)",
      "constraint": ["UNIQUE", "NOT NULL"]
    },
    "name": {
      "type": "CHAR(16)",
      "constraint": ["NOT NULL"]
    },
    "introduce": {
      "type": "VARCHAR(128)",
      "constraint": ["DEFAULT NULL"]
    },
    "avatar": {
      "type": "TEXT",
      "constraint": ["DEFAULT NULL"]
    },
    "salt": {
      "type": "CHAR(64)",
      "constraint": ["DEFAULT NULL"]
    },
    "hash": {
      "type": "CHAR(64)",
      "constraint": ["CHAR(64)", "DEFAULT NULL"]
    },
    "valid": {
      "type": "BOOLEAN",
      "constraint": ["DEFAULT NULL"]
    },
    "secret": {
      "type": "CHAR(8)",
      "constraint": ["DEFAULT NULL"]
    },
    "naverId": {
      "type": "CHAR(8)",
      "constraint": ["DEFAULT NULL"]
    },
    "googleId": {
      "type": "CHAR(24)",
      "constraint": ["DEFAULT NULL"]
    },
    "createdAt": {
      "type": "TIMESTAMP",
      "constraint": ["NOT NULL", "DEFAULT CURRENT_TIMESTAMP"]
    }
  },
  "constraint": {
    "PRIMARY KEY": ["id", "email"]
  }
}
